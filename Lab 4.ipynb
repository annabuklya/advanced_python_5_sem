    "\n",
    "def decorator(author):\n",
    "    \n",
    "    \n",
    "    def wrapper(func):\n",
    "        \n",
    "        func.author = author\n",
    "        return func\n",
    "    \n",
    "    return wrapper\n",
    "\n",
    "\n",
    "@decorator(\"Anna\") \n",
    "\n",
    "def add2(num: int) -> int:\n",
    "    \n",
    "    return num + 2\n",
    "\n",
    "print(add2.author)"
